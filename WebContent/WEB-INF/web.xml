  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>StartCaptchaServlet2</servlet-name>
    <url-pattern>/gt/register2</url-pattern>
  </servlet-mapping>
  <filter>
    <filter-name>EncodingFilter</filter-name>
    <filter-class>filter.EncodingFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>EncodingFilter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
</web-app>